webrpc = v1

name = access-control
version = v0.1.0

enum SessionType: uint16
  - Public
  - Wallet
  - AccessKey
  - Project
  - User
  - Admin
  - Service

enum UserPermission: uint16
  - UNAUTHORIZED
  - READ
  - READ_WRITE
  - ADMIN

error 1 Unauthorized     "Unauthorized access" HTTP 401
error 2 PermissionDenied "Permission denied"   HTTP 403
error 3 SessionExpired   "Session expired"     HTTP 403
