webrpc = v1

name = access-control
version = v0.1.0

enum SessionType: uint16
  - Public
  - Wallet
  - AccessKey
  - Project
  - User
  - Admin
  - Service


error 0001 Unauthorized      "Unauthorized access"                                       HTTP 401
error 0002 PermissionDenied  "Permission denied"                                         HTTP 403
error 0003 SessionExpired    "Session expired"                                           HTTP 403
